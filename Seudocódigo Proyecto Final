 			printf("\n\tCada vez que suene el programa se cambiara al siguiente ejercicio correspondiente\n");
 			printf("\tPara comenzar con el cronometro presione cualquier tecla:");
 			system("pause -> NULL");
			//Comando para el uso del cronometro
			while(1){
 				segundos++;
 				if(segundos==60){
 					segundos=0;
 					minutos++;
				}		
				system("cls");
				printf("\n\t[%.2d:%.2d]",minutos,segundos);
				if(segundos==59){
					printf("\a");
					printf("\n\tCAMBIO DE EJERCICIO");
				}
				if(minutos==30){ //Condicional de que en 30 minutos el cronometro pare
				break;
				}
				Sleep(1000);
				}
				printf("\n\tHA TERMINADO LA RUTINA DE EJERCICIO\n\n\t");
				printf("Espero que la rutina le haya gustado y sea favorable para tener buenos resultados\n\n");
				printf("\tMuchas gracias por utilizar el programa, espero verlo pronto");
				system("pause -> NULL");
				system("cls"); //Limpieza de la pantalla
				system("color 1F"); //Cambio de color de fondo y letra
				printf("\n\n\tPrograma realizado por Ramirez Esquivel Iann Emmanuel\t\tFacultad de Ingeria");
				return 0;
		}
		//Dia Domingo
		if(op1==7){ //Pierna y gluteo
 			
 			system("cls"); //Limpieza de la pantalla
 			
 			system("color BF"); //Cambio de color de fondo y letra
 			
 			printf("\n\t\t\t\t\tDOMINGO\n\n");
 			printf("\tRutina de Pierna y Gluteo\n");
 			printf("\n\n\tSe hará la rutina mediante 30 minutos en intervalos de un minuto\n");
 			printf("\tA continuacion se le presentara un serie de ejecicios que podra realizar\n\n"); 
 			printf("\tPara comenzar presione cualquier tecla:");
 			system("pause -> NULL");
 			system("cls"); //Limpieza de la pantalla
 			printf("\n\tA continuacion se le presentara los ejercicios asignados por cada numero\n\n");
			//Ejercicios asignados	
 			printf("\t1)Squat: https://www.youtube.com/watch?v=beSvHVN8pyc\n");
			printf("\t2)Squat Jump: https://www.youtube.com/watch?v=CVaEhXotL7M\n");
			printf("\t3)Escuadras: https://www.youtube.com/watch?v=qfKHl9_EvWc I\n");
			printf("\t4)Silla electrica: https://www.youtube.com/watch?v=svWccC4cMhw\n");
			printf("\t5)Rebotes: https://www.youtube.com/watch?v=mbFMbrKMsdo\n");
			printf("\t6)Elevaciones de talon: https://www.youtube.com/watch?v=kT-daLh6V2k\n");
			printf("\t7)Metralletas: https://www.youtube.com/watch?v=C1HzU6bFmcg\n");
			printf("\t8)Impulsos Pelvicos Mayores: https://www.youtube.com/watch?v=hIxCdxMB53k\n");
			printf("\t9)Criss Cross Squat Jump: https://www.youtube.com/watch?v=pGNTs-oj4eI\n");
			printf("\t10)Sentadilla sumo: https://www.youtube.com/watch?v=QWRlcesV2MU\n");
 			
 			printf("\n\tSe realizaran los ejercicios segun los numeros proporcionados por el programa\n");
 			//Commando para la seleccion de numeros al azar
 			srand(time(NULL));
 			for(int i=0;i<=5;i++){
 					v[i]=1+rand()%10;
 					printf("\t%d\n",v[i]);
 			}
 			printf("\n\tSe repetira el circuito de ejercicios un total de 6 veces\n");
 			printf("\n\tCada vez que suene el programa se cambiara al siguiente ejercicio correspondiente\n");
 			printf("\tPara comenzar con el cronometro presione cualquier tecla:");
 			system("pause -> NULL");
			//Comando para el uso del crometro
			while(1){
 				segundos++;
 				if(segundos==60){
 					segundos=0;
 					minutos++;
				}		
				system("cls");
				printf("\n\t[%.2d:%.2d]",minutos,segundos);
				if(segundos==59){
					printf("\a");
					printf("\n\tCAMBIO DE EJERCICIO");
				}
				if(minutos==30){ //Condicional de que en 30 minutos el cronometro pare
				break;
				}
				Sleep(1000);
				}
				printf("\n\tHA TERMINADO LA RUTINA DE EJERCICIO\n\n\t");
				printf("Espero que la rutina le haya gustado y sea favorable para tener buenos resultados\n\n");
				printf("\tMuchas gracias por utilizar el programa, espero verlo pronto");
				system("pause -> NULL");
				system("cls"); //Limpieza de la pantalla
				system("color 1F"); //Cambio de color de fondo y letra
				printf("\n\n\tPrograma realizado por Ramirez Esquivel Iann Emmanuel\t\tFacultad de Ingeria");
				return 0;
		}
	//Caso c
	case 'c':
		printf("Gracias y que tenga un buen dia.");
		break;
	}
	system("pause");

return 0;
}
